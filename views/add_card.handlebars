<div class="row"></div>
<div class="container">
    <div class="row">
        <h1>Add New Card</h1>
    </div>
</div>
<div class="container">
    <div class="row">
        <form>
            <div class="uk-margin">
                <input class="uk-input uk-form-width-medium uk-form-large search-pokemon" type="text"
                    placeholder="Large" name="search-pokemon">
            </div>
        </form>
        <button class="uk-button uk-button-primary uk-button-large" id="search-pokemon-btn" for="search-pokemon">Large
            button</button>
        <form>
            <label for="cards">Filter:</label>
            <select name="filter" id="filter">
                <option value="rarity">Rarity</option>
                <option value="set">Set</option>
            </select>
        </form>
    </div>
</div>
<div class="container">
    <row>
        <h1>CARDS WILL GENERATE HERE</h1>
        {{#each pokeObjects}}
        <img src="{{imageSRC}}"> // should display all pokemon images
        {{/each}}
    </row>
</div>
<div class="row"></div>
<script type="text/javascript" src="../assets/js/add_card.js"></script>